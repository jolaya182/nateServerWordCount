{
  "name": "nateserverwordcount",
  "version": "1.0.0",
  "description": "client server application that submits a ulr and text document and returns a word count of that text document",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom",
    "testJest": "jest src/tests/sum --notify --config=jest.config.js",
    "testP": " node ./src/tests/puppeteer/example.js",
    "gql": "node ./src/gql/server.js",
    "postinstall": "npm start",
    "runserver": "(cd ./src &&  nodemon  ./server)",
    "prestart": "(cd ./src && start nodemon  ./server)  ",
    "start": " webpack serve   --hot --env PLATFORM=local --env VERSION=stag --mode development --config config/webpack.base.config.js --history-api-fallback --open Chrome.exe",
    "startwebapplication": " webpack serve  --hot --env PLATFORM=local --env VERSION=stag --mode development --config config/webpack.base.config.js --history-api-fallback --open Chrome.exe",
    "poststart": "",
    "prebuild": "webpack --mode production --config config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "lint": "eslint --debug src/. ",
    "lintFix": "eslint --debug src/. --fix",
    "prettycheck": "prettier  --check src/components/App.jsx",
    "pretty": "prettier src/** --write --only-changed ",
    "prettystage": "pretty-quick --staged",
    "husky": {
      "hooks": {
        "pre-commit": "pretty-quick --staged"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jolaya182/nateServerWordCount.git"
  },
  "author": "javier o",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jolaya182/nateServerWordCount/issues"
  },
  "homepage": "https://github.com/jolaya182/nateServerWordCount#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-bootstrap": "^1.5.2",
    "sass-loader": "^11.0.1",
